{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.Core/Stryker.Core/schema/stryker.schema.json",
  "stryker-config": {
    "reporters": ["html", "progress"],
    "verbosity": "info",
    "mutate": [
      "!**/bin/**",
      "!**/obj/**",
      "**/*.cs"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 50
    }
  }
}
